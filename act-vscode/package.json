    "Alan Candido <brodao@gmail.com> (https://bitbucket.org/brodao)"
  ],
  "files": [
    "build"
  ],
  "bin": {
    "act-wizard": "./build/index.js"
  },
  "scripts": {
    "prepublishOnly": "npm run clean && yarn run build",
    "lint": "eslint .",
    "test": "jest",
    "watch": "npm run build:dev -w",
    "build:dev": "ncc build ./src/index.ts -o build/",
    "build": "ncc build ./src/index.ts -o build/ --minify --no-cache --no-source-map-register",
    "clean": "rimraf ./build/ && rimraf ./node_modules/"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@expo/json-file": "^8.2.22",
    "@types/fs-extra": "^9.0.1",
    "@types/getenv": "^1.0.0",
    "@types/node": "^14.0.27",
    "@types/prompts": "^2.0.8",
    "@types/tar": "4.0.3",
    "@zeit/ncc": "^0.22.3",
    "babel-jest": "^26.0.1",
    "chalk": "^4.0.9",
    "commander": "^6.0.0",
    "eslint": "^7.6.0",
    "eslint-config-universe": "^4.0.0",
    "execa": "^4.0.1",
    "fs-extra": "^9.0.0",
    "getenv": "^1.0.0",
    "got": "^11.1.3",
    "gulp": "^4.0.2",
    "jest": "^26.0.1",
    "minipass": "^3.1.1",
    "ora": "^4.0.3",
    "prettier": "^2.0.5",
    "prompts": "^2.3.2",
    "tar": "^6.0.1",
    "terminal-link": "^2.1.1",
    "typescript": "^3.9.7",
    "update-check": "^1.5.4"
  },
  "config": {
    "repos": [
      {
        "id": "act",
        "label": "Assistentes AC Tools",
        "url": "https://bitbucket.org/brodao/actools-vscode/src/",
        "path": "wizards​"
      },
      {
        "id": "act-local",
        "label": "Assistentes locais",
        "url": "file://$USER_HOME/actools/",
        "path": "wizards​"
      }
    ]
  },
  "dependencies": {
    "@brodao/act-nodejs": "^1.1.1-nb.1"
  }
}
